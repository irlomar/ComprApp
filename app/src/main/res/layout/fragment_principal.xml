<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".controlador.Principal">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="25dp"
        android:layout_marginBottom="25dp"
        android:layout_marginRight="25dp"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:src="@mipmap/ic_logoaplicacion" />

        <Button
            android:id="@+id/btnListPrincipalId"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:text="Listas de la compra" />

        <Button
            android:id="@+id/btnEscPrincId"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Escaner"
            android:layout_marginBottom="10dp"/>

        <Button
            android:id="@+id/btnInventPrincId"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/Inventario" />

    </LinearLayout>

</FrameLayout>